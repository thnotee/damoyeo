
<script>

    if ('@TempData["successMsg"]') {
        Swal.fire({
            position: "top-end",
            icon: "success",
            title: '@TempData["successMsg"]',
            showConfirmButton: false,
            timer: 1500
        });
    }

    if ('@TempData["errorMsg"]') {
        Swal.fire({
            position: "top-end",
            icon: "error",
            title: '@TempData["errorMsg"]',
            showConfirmButton: false,
            timer: 1500
        });
    }
</script>